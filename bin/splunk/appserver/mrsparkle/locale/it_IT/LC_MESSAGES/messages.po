msgstr "Ora fine"

#: search_mrsparkle/modules/search/TimeRangePicker.html:148
msgid "Earliest time unit selection"
msgstr "Selezione dell'unità per ora inizio"

#: search_mrsparkle/modules/search/TimeRangePicker.html:153
msgid "Second(s) ago"
msgstr "Secondo/i fa"

#: search_mrsparkle/modules/search/TimeRangePicker.html:154
msgid "Minute(s) ago"
msgstr "Minuto/i fa"

#: search_mrsparkle/modules/search/TimeRangePicker.html:155
